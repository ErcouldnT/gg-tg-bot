{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src",
    "build": "rm -rf build && tsc",
    "start": "node build",
    "check:types": "tsc --noEmit",
    "lint": "eslint src/**/* test/**/*",
    "format": "eslint src/**/* test/**/* --fix",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "coverage": "vite preview --outDir ./coverage",
    "prepare": "husky install",
    "update": "ncu -u && yarn"
  },
  "lint-staged": {
    "src/**/*": "eslint --fix",
    "test/**/*": "eslint --fix",
    "*.ts": "tsc-files --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.11.13",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.14",
    "prettier": "^3.2.4",
    "tsc-files": "^1.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "express": "^4.18.2"
  }
}
